<?if($_REQUEST['cookie']) {	if(strlen($_REQUEST['cookie']) > 32) $user = renderUser(substr($_REQUEST['cookie'],32),'member');	else $user = renderUser($_REQUEST['cookie'],'guest');}else {	if(!$_REQUEST['name']) $_REQUEST['name'] = $_SESSION['userName'];	$user = renderUser(str_replace('***','-',$_REQUEST['name']),'userName,member');}if($user) $user += renderUserLocation($user);if (!$user) redirect($_SESSION['lastPage'],'The user who\'s profile you attempted to view does not exist.');if($_SESSION['userName'] == $user['userName']) $links .= ' - <a href="editprofile">Edit My Profile</a>';if($user['userPower']) {	setPageConst('title','ATI Profile for '.$user['userName'].'!');	setPageConst('bct','<a href="/index">Home</a> > <a href="/user/index">ATI Tools</a> > Viewing User Profile for '.$user['userName'].'');		setPage(	pFunc('block','ATI Profile for '.$user['userName'].':','	<div class="lbar">		<a href="'.$_SESSION['lastPage'].'"><< Back to last page</a><!-- - <a href="users"><< View All Users</a>-->'.$links.'	</div>		<h3>ATI Profile for '.$user['userName'].':</h3>		<table class="outer light">	  <tr>		<th colspan="2">User Details:</th>	  </tr>	  <tr>		<td class="lcol">User Name:</td>		<td class="rcol">'.$user['userName'].'</td>	  </tr>	  <tr>		<td class="lcol">Full Name:</td>		<td class="rcol">'.$user['userRealName'].'</td>	  </tr>	  <tr>		<td class="lcol">Contact:</td>		<td class="rcol"><a title="Send a Message to '.$user['userName'].'" href="/user/pm-mode-send-username-'.encrypt($user['userName'],'url').'">Â» Send a Message to '.$user['userName'].'</a></td>	  </tr>	  <tr>		<td class="lcol">Bio:</td>		<td class="rcol">'.$user['userBio'].'</td>	  </tr>	  '.($user['userSignupDate'] ? '<tr>		<td class="lcol">Registration Date:</td>		<td class="rcol">'.doDate($user['userSignupDate']).'</td>	  </tr>' : '').'	  <!--<tr>		<td class="lcol">Power:</td>		<td class="rcol">'.$user['userPower'].'</td>	  </tr>-->	  <tr>		<td class="lcol">Location:</td>		<td class="rcol">'.$user['userLocation'].'</td>	  </tr>	  <tr>		<td class="lcol">Last Date:</td>	  <td class="rcol">'.doDate($user['userLastDate']).'</td>	  </tr>	  <tr>		<td class="lcol">Hits:</td>		<td class="rcol">'.$user['userHits'].'</td>	  </tr>	  <tr>		<td class="lcol">Visits:</td>		<td class="rcol">'.$user['userVisits'].'</td>	  </tr>	  <tr>		<td class="lcol">Time:</td>		<td class="rcol">'.dateOffset($user['userTime']).'</td>	  </tr>	</table>	'));	}else {		setPageConst('title','ATI Guest Profile!');	setPageConst('bct','<a href="/index">Home</a> > <a href="/user/index">ATI Tools</a> > Viewing Guest Profile');		setPage(	pFunc('block','ATI Guest Profile:','	<div class="lbar">		<a href="'.$_SESSION['lastPage'].'"><< Back to last page</a>'.$links.'	</div>		<h3>ATI Guest Profile:</h3>	<table class="outer light">	  <tr>		<th colspan="2">User Details:</th>		<!--<th>User Activity:</th>-->	  </tr>	  <tr>		<td class="lcol">Registration Date:</td>		<td class="rcol">'.doDate($user['userSignupDate']).'</td>	  </tr>	  <tr>		<td class="lcol">Location:</td>		<td class="rcol">'.$user['userLocation'].'</td>	  </tr>	  <tr>		<td class="lcol">Last Date:</td>	  <td class="rcol">'.doDate($user['userLastDate']).'</td>	  </tr>	  <tr>		<td class="lcol">Hits:</td>		<td class="rcol">'.$user['userHits'].'</td>	  </tr>	  <tr>		<td class="lcol">Visits:</td>		<td class="rcol">'.$user['userVisits'].'</td>	  </tr>	  <tr>		<td class="lcol">Time:</td>		<td class="rcol">'.dateOffset($user['userTime']).'</td>	  </tr>	</table>	'));}?>